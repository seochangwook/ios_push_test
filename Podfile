# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'testapp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Alamofire network
	pod 'Alamofire', '~> 4.5'

  # Pods for SwiftyJSON
	pod 'SwiftyJSON'

  # Pods for Kingfisher
	pod 'Kingfisher', '~>4.0'

  # Pods for Firebase
	pod 'Firebase/Core'
	pod 'Firebase/Messaging'

  # Pods for testapp

  target 'testappTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'testappUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
